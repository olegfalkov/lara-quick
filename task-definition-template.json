{
    "containerDefinitions": [
      {
        "name": "web-app-container",
        "image": "885193784904.dkr.ecr.us-east-2.amazonaws.com/deployment-demo-registry:#{Octopus.Release.Number}",
        "cpu": 0,
        "memoryReservation": 256,
        "portMappings": [
          {
            "containerPort": 80,
            "hostPort": 80,
            "protocol": "tcp"
          }
        ],
        "essential": true,
        "command": [ "start-apache" ],
        "environment": [
          {
            "name": "APP-ENV",
            "value": "#{EnvironmentName}"
          }
        ],
        "secrets": [
          {
            "name": "DB_PASSWORD",
            "valueFrom": "arn:aws:secretsmanager:us-east-2:885193784904:secret:#{EnvironmentName}/password"
          },
          {
            "name": "DB_USERNAME",
            "valueFrom": "arn:aws:secretsmanager:us-east-2:885193784904:secret:#{EnvironmentName}/username"
          }
        ],
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "/ecs/lara-quick-log",
            "awslogs-region": "us-east-2",
            "awslogs-stream-prefix": "ecs"
          }
        }
      }
    ],
    "family": "#{EnvironmentName}-lara-quick_v1-2-57",
    "executionRoleArn": "arn:aws:iam::885193784904:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "status": "ACTIVE",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512",
    "taskRoleArn": "arn:aws:iam::885193784904:role/ecsTaskExecutionRole"
}
